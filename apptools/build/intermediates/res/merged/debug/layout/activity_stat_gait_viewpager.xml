<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:bar="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp">
        <ImageView
            android:id="@+id/gait_left"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/gait_left_icon_2"
            android:layout_marginLeft="@dimen/layout_marginLeft"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:src="@drawable/gait_right_icon_2"
            android:id="@+id/imageView"
            android:layout_marginRight="@dimen/layout_marginRight"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="左脚"
            android:id="@+id/textView3"
            android:layout_toEndOf="@+id/gait_left"
            android:layout_centerVertical="true"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="右脚"
            android:id="@+id/textView4"
            android:layout_alignParentTop="true"
            android:layout_toStartOf="@+id/imageView"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_marginLeft="@dimen/layout_marginLeft"
        android:layout_marginRight="@dimen/layout_marginRight"
        android:layout_marginTop="@dimen/layout_marginTop"
        >
        <TextView
            android:id="@+id/leftInnerText"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textSize="12sp"
            android:text="0"/>
        <com.advanpro.fwtools.module.stat.GaitProgressBar
            android:id="@+id/leftInner"
            android:layout_width="match_parent"
            android:layout_height="@dimen/gait_bar_size"
            android:layout_weight="1"
            bar:startColor="@color/a2"
            bar:stopColor="@color/a1"
            />
        <com.advanpro.fwtools.module.stat.GaitProgressBar
            android:id="@+id/rightInner"
            android:layout_width="match_parent"
            android:layout_height="@dimen/gait_bar_size"
            android:layout_weight="1"
            android:rotationY="180"
            bar:startColor="@color/a3"
            bar:stopColor="@color/a4"
            />
        <TextView
            android:id="@+id/rightInnerText"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:gravity="center"
            android:textSize="12sp"
            android:text="0"/>
        <!--<TextView-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:text="%"/>-->
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center"
        >
        <TextView
            android:id="@+id/innerText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="内翻"
            android:textColor="@color/tab_view_normal_text"/>

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_marginLeft="@dimen/layout_marginLeft"
        android:layout_marginRight="@dimen/layout_marginRight"
        android:layout_marginTop="@dimen/layout_marginTop"
        >
        <TextView
            android:id="@+id/leftOutText"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textSize="12sp"
            android:text="0"/>
        <!--<TextView-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_marginRight="12dp"-->

            <!--android:text="%"/>-->
        <com.advanpro.fwtools.module.stat.GaitProgressBar
            android:id="@+id/leftOut"
            android:layout_width="match_parent"
            android:layout_height="@dimen/gait_bar_size"
            android:layout_weight="1"
            bar:startColor="@color/a2"
            bar:stopColor="@color/a1"
            />
        <com.advanpro.fwtools.module.stat.GaitProgressBar
            android:id="@+id/rightOut"
            android:layout_width="match_parent"
            android:layout_height="@dimen/gait_bar_size"
            android:layout_weight="1"
            android:rotationY="180"
            bar:startColor="@color/a3"
            bar:stopColor="@color/a4"
            />
        <TextView
            android:id="@+id/rightOutText"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:gravity="center"
            android:textSize="12sp"
            android:text="0"/>
        <!--<TextView-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:text="%"/>-->
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center"
        >
        <TextView
            android:id="@+id/outText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="外翻"
            android:textColor="@color/tab_view_normal_text"/>

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_marginLeft="@dimen/layout_marginLeft"
        android:layout_marginRight="@dimen/layout_marginRight"
        android:layout_marginTop="@dimen/layout_marginTop"
        >
        <TextView
            android:id="@+id/leftNormalText"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textSize="12sp"
            android:text="0"/>
        <!--<TextView-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_marginRight="10dp"-->
            <!--android:text="%"/>-->
        <com.advanpro.fwtools.module.stat.GaitProgressBar
            android:id="@+id/leftNormal"
            android:layout_width="match_parent"
            android:layout_height="@dimen/gait_bar_size"
            android:layout_weight="1"
            bar:startColor="@color/a2"
            bar:stopColor="@color/a1"
            />
        <com.advanpro.fwtools.module.stat.GaitProgressBar
            android:id="@+id/rightNormal"
            android:layout_width="match_parent"
            android:layout_height="@dimen/gait_bar_size"
            android:layout_weight="1"
            android:rotationY="180"
            bar:startColor="@color/a3"
            bar:stopColor="@color/a4"
            />
        <TextView
            android:id="@+id/rightNormalText"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:gravity="center"
            android:textSize="12sp"
            android:text="0"/>
        <!--<TextView-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:text="%"/>-->
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="正常前脚掌着地"
            android:textColor="@color/tab_view_normal_text"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_marginLeft="@dimen/layout_marginLeft"
        android:layout_marginRight="@dimen/layout_marginRight"
        android:layout_marginTop="@dimen/layout_marginTop"
        >
        <TextView
            android:id="@+id/leftAllText"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textSize="12sp"
            android:text="0"/>
        <!--<TextView-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_marginRight="10dp"-->
            <!--android:text="%"/>-->
        <com.advanpro.fwtools.module.stat.GaitProgressBar
            android:id="@+id/leftAll"
            android:layout_width="match_parent"
            android:layout_height="@dimen/gait_bar_size"
            android:layout_weight="1"
            bar:startColor="@color/a2"
            bar:stopColor="@color/a1"
            />
        <com.advanpro.fwtools.module.stat.GaitProgressBar
            android:id="@+id/rightAll"
            android:layout_width="match_parent"
            android:layout_height="@dimen/gait_bar_size"
            android:layout_weight="1"
            android:rotationY="180"
            bar:startColor="@color/a3"
            bar:stopColor="@color/a4"
            />
        <TextView
            android:id="@+id/rightAllText"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:gravity="center"
            android:textSize="12sp"
            android:text="0"/>
        <!--<TextView-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:text="%"/>-->
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="全脚着地"
            android:textColor="@color/tab_view_normal_text"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_marginLeft="@dimen/layout_marginLeft"
        android:layout_marginRight="@dimen/layout_marginRight"
        android:layout_marginTop="@dimen/layout_marginTop"
        >
        <TextView
            android:id="@+id/leftText"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textSize="12sp"
            android:text="0"/>
        <!--<TextView-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_marginRight="10dp"-->
            <!--android:text="%"/>-->
        <com.advanpro.fwtools.module.stat.GaitProgressBar
            android:id="@+id/left"
            android:layout_width="match_parent"
            android:layout_height="@dimen/gait_bar_size"
            android:layout_weight="1"
            bar:startColor="@color/a2"
            bar:stopColor="@color/a1"
            />
        <com.advanpro.fwtools.module.stat.GaitProgressBar
            android:id="@+id/right"
            android:layout_width="match_parent"
            android:layout_height="@dimen/gait_bar_size"
            android:layout_weight="1"
            android:rotationY="180"
            bar:startColor="@color/a3"
            bar:stopColor="@color/a4"
            />
        <TextView
            android:id="@+id/rightText"
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:gravity="center"
            android:textSize="12sp"
            android:text="0"/>
        <!--<TextView-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:text="%"/>-->
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="脚跟着地"
            android:textColor="@color/tab_view_normal_text"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="@drawable/linear_bg"
        android:layout_marginLeft="28dp"
        android:layout_marginRight="28dp"
        android:layout_marginBottom="20dp"
        android:layout_marginTop="20dp"
        android:orientation="vertical"
        android:visibility="gone">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_weight="1">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/gait_value_2"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="分析："
                android:textSize="@dimen/stat_size"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="15dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="top|center"
            android:layout_weight="2">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""/>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>
