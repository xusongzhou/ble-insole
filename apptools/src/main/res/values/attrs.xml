<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleBar">
        <attr name="title" format="string|reference"/>
        <attr name="startTextViewVisible" format="boolean"/>
        <attr name="endTextViewVisible" format="boolean"/>
        <attr name="startImageButtonVisible" format="boolean"/>
        <attr name="endImageButtonVisible" format="boolean"/>
        <attr name="startText" format="string|reference"/>
        <attr name="endText" format="string|reference"/>
        <attr name="startImageButtonSrc" format="reference"/>
        <attr name="endImageButtonSrc" format="reference"/>
    </declare-styleable>
    <declare-styleable name="SwitchLayout">
        <attr name="orientation" format="enum">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
            <enum name="top" value="2"/>
            <enum name="bottom" value="3"/>
        </attr>
        <attr name="duration" format="integer"/>
    </declare-styleable>
    <declare-styleable name="SwitchButton">
        <attr name="swThumbDrawable" format="reference"/>
        <attr name="swThumbColor" format="color|reference"/>
        <attr name="swThumbMargin" format="dimension|reference"/>
        <attr name="swThumbMarginTop" format="dimension|reference"/>
        <attr name="swThumbMarginBottom" format="dimension|reference"/>
        <attr name="swThumbMarginLeft" format="dimension|reference"/>
        <attr name="swThumbMarginRight" format="dimension|reference"/>
        <attr name="swThumbWidth" format="dimension|reference"/>
        <attr name="swThumbHeight" format="dimension|reference"/>
        <attr name="swThumbRadius" format="dimension|reference"/>
        <attr name="swBackRadius" format="dimension|reference"/>
        <attr name="swBackDrawable" format="reference"/>
        <attr name="swBackColor" format="color|reference"/>
        <attr name="swFadeBack" format="boolean"/>
        <attr name="swBackMeasureRatio" format="float"/>
        <attr name="swAnimationDuration" format="integer"/>
        <attr name="swTintColor" format="color|reference"/>
    </declare-styleable>
    <declare-styleable name="ArrowItemView">
        <attr name="itemTitle" format="string|reference"/>
        <attr name="itemTitleColor" format="color|reference"/>
        <attr name="arrowColor" format="color|reference"/>
        <attr name="hint" format="string|reference"/>
        <attr name="hintVisible" format="boolean"/>
        <attr name="topDividerVisible" format="boolean"/>
        <attr name="bottomDividerVisible" format="boolean"/>
        <attr name="switchChecked" format="boolean"/>
        <attr name="style" format="enum">
            <enum name="arrow" value="0"/>
            <enum name="toggle" value="1"/>
            <enum name="nothing" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="GaitProgressBar">
        <attr name="startColor" format="color"/>
        <attr name="stopColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="BrokenLineView">
        <attr name="type" format="string"/>
    </declare-styleable>
    <declare-styleable name="BrokenLinePillar">
        <attr name="pillar" format="string"/>
    </declare-styleable>
    <declare-styleable name="HorizontalChildrenView">
        <attr name="myviewType" format="integer"/>
    </declare-styleable>
    <declare-styleable name="PostView">
        <attr name="color" format="color"/>
    </declare-styleable>
</resources>